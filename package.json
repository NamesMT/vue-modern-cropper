{
  "name": "vue-modern-cropper",
  "description": "Power-packed wrapper over cropperjs@next",
  "version": "0.0.1",
  "type": "module",
  "author": "NamesMT <dangquoctrung123@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build:docs": "vite build --mode docs",
    "build:lib": "vite build --mode lib && vue-tsc -p tsconfig.build.json",
    "preview": "vite preview",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NamesMT/vue-modern-cropper.git"
  },
  "homepage": "https://github.com/NamesMT/vue-modern-cropper",
  "files": [
    "lib"
  ],
  "main": "./lib/vue-modern-cropper.umd.cjs",
  "module": "./lib/vue-modern-cropper.js",
  "exports": {
    ".": {
      "import": "./lib/vue-modern-cropper.js",
      "require": "./lib/vue-modern-cropper.umd.cjs",
      "types": "./lib/index.d.ts"
    }
  },
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@iconify/json": "^2.2.177",
    "@types/node": "^18.19.14",
    "@unocss/reset": "^0.55.7",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vueuse/core": "^10.7.2",
    "clean-css": "^5.3.3",
    "highlight.js": "^11.9.0",
    "sass": "^1.70.0",
    "typescript": "^4.9.5",
    "unocss": "^0.55.7",
    "unplugin-icons": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.5.2",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  }
